@using Bennington.Cms.ReportManagement.Models
@using Bennington.Cms.ReportManagement.Controllers
@model ReportListViewModel

@if (!string.IsNullOrWhiteSpace(Model.UrlToStreamCsv))
{
    <a href="#" onclick="window.location='@Html.Raw(Model.UrlToStreamCsv)' + '?id=@Model.Id&StartDate=' + $('#StartDate' + '_' +'@Model.Id').val() + '&EndDate=' + $('#EndDate' + '_' + '@Model.Id').val();return(false);">View</a>
}
else
{
    <a onclick="window.location='@Url.Action("Stream", typeof(ReportManagementController).Name.Replace("Controller", string.Empty), new { id = Model.Id })' + '&StartDate=' + $('#StartDate' + '_' +'@Model.Id').val() + '&EndDate=' + $('#EndDate' + '_' + '@Model.Id').val();return(false);" href="#">View</a>
}